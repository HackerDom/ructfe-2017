server {
	listen 80 default_server;

	server_name localhost;

	location / {
		autoindex off;
		root /home/raster/static;
	}

	location /add_ship {
		proxy_pass http://127.0.0.1:16780;
	}

	location /draw {
		proxy_pass http://127.0.0.1:16780;
	}

	location /get_shader {
		proxy_pass http://127.0.0.1:16780;
	}
}